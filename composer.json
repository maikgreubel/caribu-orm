{
	"keywords" : [
		"ORM"
	],
	"autoload" : {
		"psr-4" : {
			"Nkey\\Caribu\\" : "src",
			"Nkey\\Caribu\\Type\\" : "src/Type",
			"Nkey\\Caribu\\Orm\\" : "src/Orm",
			"Nkey\\Caribu\\Model\\" : "src/Model"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Nkey\\Caribu\\Tests\\Model\\" : "tests/Model",
			"Nkey\\Caribu\\Tests\\" : [
				"tests/complex-tests",
				"tests/failure-tests",
				"tests/simple-tests",
				"tests"
			]
		}
	},
	"name" : "nkey/caribu",
	"type" : "library",
	"description" : "Annotation based OR Mapping framework for PHP",
	"license" : "BSD-3-Clause",
	"require" : {
		"php" : "~7.0",
		"psr/log" : "~1.0",
		"ext-pdo" : "*",
		"ext-Reflection" : "*",
		"ext-date" : "*",
		"nkey/phpgenerics" : "dev-master"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~6.0",
		"phpunit/dbunit" : "~3.0",
		"squizlabs/php_codesniffer" : "~2.0",
		"phploc/phploc" : "~3.0",
		"pdepend/pdepend" : "~2.0",
		"phpmd/phpmd" : "~2.0",
		"sebastian/phpcpd" : "~3.0",
		"theseer/phpdox" : "~0.8",
		"scrutinizer/ocular" : "~1.3"
	},
	"support" : {
		"email" : "greubel@nkey.de",
		"issues" : "https://github.com/maikgreubel/caribu-orm/issues",
		"wiki" : "https://github.com/maikgreubel/caribu-orm/wiki",
		"source" : "https://github.com/maikgreubel/caribu-orm"
	},
	"authors" : [{
			"name" : "Maik Greubel",
			"email" : "greubel@nkey.de",
			"homepage" : "https://nkey.de",
			"role" : "Maintainer"
		}
	],
	"homepage" : "https://github.com/maikgreubel/caribu-orm",
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.0-dev"
		}
	},
	"minimum-stability" : "dev"
}